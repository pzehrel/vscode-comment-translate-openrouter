{
  "publisher": "pzehrel",
  "name": "openrouter-translate",
  "displayName": "openrouter-translate",
  "version": "0.0.1",
  "packageManager": "pnpm@10.12.4",
  "description": "Openrouter translation source for comment-translate",
  "homepage": "https://github.com/pzehrel/vscode-comment-translate-openrouter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/pzehrel/vscode-comment-translate-openrouter.git"
  },
  "bugs": {
    "url": "https://github.com/pzehrel/vscode-comment-translate-openrouter/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.101.0",
    "node": "20"
  },
  "extensionDependencies": [
    "intellsmi.comment-translate"
  ],
  "activationEvents": [],
  "contributes": {
    "configuration": [
      {
        "title": "Openrouter translation configuration",
        "type": "Object",
        "properties": {
          "openrouterTranslate.apiKey": {
            "type": "string",
            "markdownDescription": "API Key [get key](https://openrouter.ai/settings/keys)"
          },
          "openrouterTranslate.model": {
            "type": "string",
            "default": "google/gemini-2.0-flash-lite-001",
            "enum": [
              "google/gemini-2.0-flash-lite-001",
              "google/gemini-2.0-flash-001",
              "deepseek/deepseek-chat-v3-0324",
              "deepseek/deepseek-chat"
            ],
            "markdownDescription": "AI model name [model list](https://openrouter.ai/models?fmt=table)"
          },
          "openrouterTranslate.api": {
            "type": "string",
            "default": "https://openrouter.ai/api/v1/chat/completions",
            "description": "API service address"
          },
          "openrouterTranslate.prompts.system": {
            "type": "string",
            "markdownDescription": "System prompt, [see document](https://github.com/pzehrel/vscode-comment-translate-openrouter)"
          },
          "openrouterTranslate.prompts.translate": {
            "type": "string",
            "markdownDescription": "Translation prompt, [see document](https://github.com/pzehrel/vscode-comment-translate-openrouter)"
          },
          "openrouterTranslate.timeout": {
            "type": "number",
            "description": "Request timeout in milliseconds"
          }
        }
      }
    ],
    "translates": [
      {
        "translate": "openrouter",
        "title": "openrouter translate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint .",
    "test": "vscode-test"
  },
  "dependencies": {
    "comment-translate-manager": "^0.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.29.0",
    "typescript": "^5.8.3"
  }
}
